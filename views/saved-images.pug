              table.table
                thead
                  tr
                    th url
                    th name
                    th host
                    th Status
                    th Actions
                tbody.table-border-bottom-0
                  tr
                    td
                      i.fab.fa-angular.fa-lg.text-danger.me-3
                      |  
                      strong url 1
                    td name 1
                    td
                      ul.list-unstyled.users-list.m-0.avatar-group.d-flex.align-items-center
                        li.avatar.avatar-xs.pull-up(data-bs-toggle='tooltip', data-popup='tooltip-custom', data-bs-placement='top', title='Lilian Fuller')
                          img.rounded-circle(src='/assets/img/avatars/5.png', alt='Avatar')
